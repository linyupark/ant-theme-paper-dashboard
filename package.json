{
  "name": "@x011/theme-paper-dashboard",
  "version": "0.0.1",
  "description": "Theme Paper Dashboard for Ant Design Web",
  "exports": {
    ".": "./dist/theme.js",
    "./style": "./dist/style.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && npx microbundle src/*.js",
    "build-tsx": "npx microbundle src/utils/index.ts -o dist/utils/index.js --jsx React.createElement -f cjs,es,modern",
    "build-less": "npx lessc src/less/index.less src/css/index.css"
  },
  "author": "linyupark@gmail.com",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "umi": "^3.4.8"
  },
  "devDependencies": {
    "less": "^4.1.1",
    "microbundle": "^0.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.4",
    "umi": "^3.4.8"
  }
}
